{
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "start": "npm run build:dev && npm run server:dev",
    "build:prod": "webpack --env.NODE_ENV=prod",
    "build:dev": "webpack --env.NODE_ENV=dev",
    "server:dev": "nodemon \"./dist/server.dev.js\" --watch \"./dist/server.dev.js\"",
    "watch:dev": "webpack --env.NODE_ENV=dev --watch",
    "server:prod": "nodemon \"./dist/server.prod.js\" --watch \"./dist/server.prod.js\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "argon2-ffi": "^2.0.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.4",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.8.0",
    "mongoose": "^7.4.0",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.0",
    "pino": "^8.2.0",
    "pino-pretty": "^8.1.0",
    "ts-loader": "^6.0.1",
    "webpack": "^4.32.2",
    "zod": "^3.17.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.4",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.0.6",
    "@types/pino": "^7.0.5",
    "@types/webpack": "^4.4.32",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^8.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "typescript": "^5.1.6",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "name": "nodejs_typescript",
